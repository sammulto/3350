<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.groupeleven.studentlife"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.StudentLife" >
        <activity
            android:name="com.groupeleven.studentlife.ui.todolist.Toupdate"
            android:parentActivityName="com.groupeleven.studentlife.ui.todolist.Toupdate" />
        <activity
            android:name="com.groupeleven.studentlife.ui.calendar.CalendarToUpdate"
            android:parentActivityName="com.groupeleven.studentlife.ui.calendar.CalendarToUpdate" />
        <activity
            android:name="com.groupeleven.studentlife.ui.usefullinks.Tutorial"
            android:parentActivityName="com.groupeleven.studentlife.ui.usefullinks.Tutorial" />
        <activity
            android:name="com.groupeleven.studentlife.ui.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="android.test.runner"
            android:required="false" />
        <uses-library
            android:name="android.test.base"
            android:required="false" />
        <uses-library
            android:name="android.test.mock"
            android:required="false" />

        <provider
            android:name="com.prolificinteractive.materialcalendarview.MaterialCalendarViewInitProvider"
            android:authorities="com.groupeleven.studentlife.materialcalendarviewinitprovider"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>